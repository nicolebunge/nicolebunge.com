{
  "name": "nicolebunge.com",
  "description": "Personal website of Nicole Bunge",
  "version": "1.0.0",
  "private": true,
  "dependencies": {
    "@fontsource/source-sans-pro": "^4.5.3",
    "@sentry/gatsby": "^6.17.3",
    "@sentry/tracing": "^6.17.3",
    "@sindresorhus/slugify": "^1.1.2",
    "clsx": "^1.1.1",
    "gatsby": "^4.24.1",
    "gatsby-plugin-google-analytics": "^4.24.0",
    "gatsby-plugin-manifest": "^4.24.0",
    "gatsby-plugin-react-helmet": "^5.24.0",
    "gatsby-plugin-sitemap": "^5.24.0",
    "gatsby-source-filesystem": "^4.24.0",
    "gatsby-transformer-json": "^4.24.0",
    "gatsby-transformer-remark": "^5.24.0",
    "lodash": "^4.17.21",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-helmet": "^6.1.0",
    "react-use": "^17.3.2"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "check": "tsc",
    "clean": "gatsby clean",
    "develop": "gatsby develop",
    "format": "prettier --ignore-path .gitignore --write .",
    "generate:svg": "svgr --icon --typescript --out-dir src/components/Icons src/images/icons",
    "generate:tokens": "style-dictionary build --config src/style-dictionary.json",
    "generate": "npm-run-all --parallel generate:*",
    "lint:css": "stylelint --ignore-path .gitignore '**/*.css'",
    "lint:js": "eslint '**/*.{js,jsx,ts,tsx}'",
    "lint": "npm-run-all --parallel lint:*",
    "serve": "gatsby serve",
    "start": "npm run serve",
    "test": "jest --maxWorkers=50% --watch",
    "test:ci": "jest --ci --coverage --runInBand"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nicolebunge/nicolebunge.com"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "@babel/preset-typescript": "^7.16.7",
    "@storybook/testing-react": "^1.2.4",
    "@svgr/cli": "^6.2.1",
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/react": "^12.1.4",
    "@types/jest": "^27.4.0",
    "@types/jest-axe": "^3.5.3",
    "@types/lodash": "^4.14.179",
    "@types/node": "^17.0.13",
    "@types/react": "^17.0.39",
    "@types/react-dom": "^17.0.11",
    "@types/react-helmet": "^6.1.5",
    "@typescript-eslint/eslint-plugin": "^5.13.0",
    "@typescript-eslint/parser": "^5.38.1",
    "eslint": "^8.8.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.5.1",
    "jest-axe": "^5.0.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.5.1",
    "style-dictionary": "^3.1.1",
    "stylelint": "^14.6.1",
    "stylelint-config-standard": "^24.0.0",
    "stylelint-order": "^5.0.0",
    "stylelint-selector-bem-pattern": "^2.1.1",
    "typescript": "^4.6.2"
  }
}

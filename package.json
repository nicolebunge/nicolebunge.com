{
  "name": "nicolebunge.com",
  "description": "Personal website of Nicole Bunge",
  "version": "1.0.0",
  "private": true,
  "dependencies": {
    "@fontsource/source-sans-pro": "^5.1.0",
    "@sindresorhus/slugify": "^1.1.2",
    "clsx": "^2.1.1",
    "gatsby": "^5.13.7",
    "gatsby-source-filesystem": "^5.13.1",
    "gatsby-transformer-json": "^5.13.1",
    "gatsby-transformer-remark": "^6.13.1",
    "lodash": "^4.17.21",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-use": "^17.5.1"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "check": "tsc",
    "clean": "gatsby clean",
    "coverage": "vitest run --coverage",
    "develop": "gatsby develop",
    "format": "prettier --ignore-path .gitignore --write .",
    "generate:svg": "npx @svgr/cli --icon --typescript --out-dir src/components/Icons src/images/icons",
    "generate": "npm-run-all --parallel generate:*",
    "lint:css": "stylelint --ignore-path .gitignore 'src/**/*.css'",
    "lint:js": "eslint 'src/**/*.{js,jsx,ts,tsx}'",
    "lint": "npm-run-all --parallel lint:*",
    "serve": "gatsby serve",
    "start": "npm run serve",
    "test": "vitest",
    "update": "npx npm-check-updates --format group --interactive"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nicolebunge/nicolebunge.com"
  },
  "devDependencies": {
    "@storybook/react": "^8.4.0",
    "@testing-library/react": "^16.0.1",
    "@types/lodash": "^4.17.13",
    "@types/node": "^20.14.11",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@typescript-eslint/eslint-plugin": "^7.16.1",
    "@typescript-eslint/parser": "^7.16.1",
    "@vitest/coverage-v8": "^2.1.4",
    "eslint": "^8.57.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.30.0",
    "eslint-plugin-jsx-a11y": "^6.10.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.37.0",
    "eslint-plugin-react-hooks": "^4.6.2",
    "happy-dom": "^15.10.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.3.3",
    "stylelint": "^16.10.0",
    "stylelint-config-standard": "^36.0.1",
    "stylelint-order": "^6.0.4",
    "stylelint-selector-bem-pattern": "^4.0.1",
    "typescript": "^5.6.3",
    "vitest": "^2.0.5"
  }
}

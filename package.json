{
  "name": "nicolebunge.com",
  "description": "Personal website of Nicole Bunge",
  "version": "1.0.0",
  "private": true,
  "dependencies": {
    "@fontsource/source-sans-pro": "^5.0.8",
    "@sindresorhus/slugify": "^1.1.2",
    "clsx": "^2.1.0",
    "gatsby": "^5.13.4",
    "gatsby-source-filesystem": "^5.13.1",
    "gatsby-transformer-json": "^5.13.1",
    "gatsby-transformer-remark": "^6.13.1",
    "lodash": "^4.17.21",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-use": "^17.5.0"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "check": "tsc",
    "clean": "gatsby clean",
    "develop": "gatsby develop",
    "format": "prettier --ignore-path .gitignore --write .",
    "generate:svg": "svgr --icon --typescript --out-dir src/components/Icons src/images/icons",
    "generate": "npm-run-all --parallel generate:*",
    "lint:css": "stylelint --ignore-path .gitignore '**/*.css'",
    "lint:js": "eslint '**/*.{js,jsx,ts,tsx}'",
    "lint": "npm-run-all --parallel lint:*",
    "serve": "gatsby serve",
    "start": "npm run serve",
    "test": "vitest",
    "test:ci": "vitest run --coverage",
    "update": "npx npm-check-updates --format group --interactive"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nicolebunge/nicolebunge.com"
  },
  "devDependencies": {
    "@storybook/react": "^7.6.17",
    "@svgr/cli": "^8.1.0",
    "@testing-library/react": "^14.2.1",
    "@types/lodash": "^4.17.0",
    "@types/node": "^20.12.7",
    "@types/react": "^18.3.1",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^7.6.0",
    "@typescript-eslint/parser": "^7.6.0",
    "@vitest/coverage-v8": "^1.6.0",
    "eslint": "^8.57.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "happy-dom": "^13.6.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.2.5",
    "stylelint": "^16.3.1",
    "stylelint-config-standard": "^36.0.0",
    "stylelint-order": "^6.0.4",
    "stylelint-selector-bem-pattern": "^3.0.1",
    "typescript": "^5.4.5",
    "vitest": "^1.5.0"
  }
}

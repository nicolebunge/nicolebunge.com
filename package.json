{
  "name": "nicolebunge.com",
  "description": "",
  "version": "1.0.0",
  "dependencies": {
    "@fontsource/source-sans-pro": "^4.5.0",
    "clsx": "^1.1.1",
    "gatsby": "^4.0.2",
    "gatsby-plugin-google-analytics": "^4.0.0",
    "gatsby-plugin-manifest": "^4.0.0",
    "gatsby-plugin-react-helmet": "^5.0.0",
    "gatsby-plugin-sentry": "^1.0.1",
    "gatsby-plugin-sitemap": "^5.0.0",
    "gatsby-source-filesystem": "^4.0.0",
    "gatsby-transformer-remark": "^5.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-helmet": "^6.1.0"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "check": "tsc --noEmit",
    "clean": "gatsby clean",
    "develop": "gatsby develop",
    "format": "prettier --ignore-path .gitignore --write .",
    "generate:svg": "svgr --icon --typescript --out-dir src/components/Icons src/images/icons",
    "generate:tokens": "style-dictionary build --config src/style-dictionary.json",
    "generate": "npm-run-all --parallel generate:svg generate:tokens",
    "lint:css": "stylelint --ignore-path .gitignore '**/*.css'",
    "lint:js": "eslint '**/*.{js,jsx,ts,tsx}'",
    "lint": "npm-run-all --parallel lint:css lint:js",
    "serve": "gatsby serve",
    "start": "npm run serve",
    "test": "jest --maxWorkers=50%",
    "test:ci": "jest --ci --coverage --runInBand"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nicolebunge/nicolebunge.com"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.15.8",
    "@babel/preset-react": "^7.16.0",
    "@babel/preset-typescript": "^7.15.0",
    "@svgr/cli": "^5.5.0",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.1.2",
    "@types/jest": "^27.0.2",
    "@types/node": "^16.10.2",
    "@types/react": "^17.0.26",
    "@types/react-dom": "^17.0.9",
    "@types/react-helmet": "^6.1.2",
    "@typescript-eslint/eslint-plugin": "^4.32.0",
    "@typescript-eslint/parser": "^4.32.0",
    "eslint": "^7.32.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.26.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.3.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.4.1",
    "style-dictionary": "^3.0.2",
    "stylelint": "^13.13.1",
    "stylelint-config-standard": "^22.0.0",
    "stylelint-order": "^4.1.0",
    "typescript": "^4.4.4"
  }
}

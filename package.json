{
  "name": "nicolebunge.com",
  "description": "Personal website of Nicole Bunge",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "next build",
    "check": "tsc",
    "coverage": "vitest run --coverage",
    "dev": "next dev",
    "format": "prettier --ignore-unknown --write .",
    "generate:svg": "npx @svgr/cli --icon --typescript --out-dir src/components/Icons src/images/icons",
    "generate": "npm-run-all --parallel generate:*",
    "lint:css": "stylelint --ignore-path .gitignore 'src/**/*.css'",
    "lint:js": "eslint",
    "lint": "npm-run-all --parallel lint:*",
    "start": "next start",
    "test": "vitest",
    "update": "npx npm-check-updates --format group --interactive"
  },
  "keywords": [
    "next"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/nicolebunge/nicolebunge.com"
  },
  "dependencies": {
    "@mdx-js/loader": "^3.1.1",
    "@mdx-js/react": "^3.1.1",
    "@next/mdx": "^16.1.6",
    "@types/mdx": "^2.0.13",
    "clsx": "^2.1.1",
    "next": "16.1.6",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "react-use": "^17.6.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@storybook/react": "^10.2.3",
    "@testing-library/react": "^16.3.2",
    "@types/node": "^24",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@vitejs/plugin-react": "^5.1.2",
    "@vitest/coverage-v8": "^4.0.18",
    "eslint": "^9",
    "eslint-config-next": "16.1.6",
    "happy-dom": "^20.4.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.8.1",
    "storybook": "^10.2.13",
    "stylelint": "^17.4.0",
    "stylelint-config-standard": "^39.0.1",
    "stylelint-order": "^7.0.1",
    "stylelint-selector-bem-pattern": "^4.0.1",
    "typescript": "^5",
    "vitest": "^4.0.6"
  }
}

{
  "name": "nicolebunge.com",
  "description": "Personal website of Nicole Bunge",
  "version": "1.0.0",
  "private": true,
  "dependencies": {
    "@fontsource/source-sans-pro": "^4.5.11",
    "@sentry/gatsby": "^7.57.0",
    "@sentry/tracing": "^7.57.0",
    "@sindresorhus/slugify": "^1.1.2",
    "clsx": "^1.2.1",
    "gatsby": "^5.11.0",
    "gatsby-plugin-google-analytics": "^5.11.0",
    "gatsby-plugin-manifest": "^5.11.0",
    "gatsby-plugin-react-helmet": "^6.11.0",
    "gatsby-plugin-sitemap": "^6.11.0",
    "gatsby-source-filesystem": "^5.11.0",
    "gatsby-transformer-json": "^5.11.0",
    "gatsby-transformer-remark": "^6.11.0",
    "lodash": "^4.17.21",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet": "^6.1.0",
    "react-use": "^17.4.0"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "check": "tsc",
    "clean": "gatsby clean",
    "develop": "gatsby develop",
    "format": "prettier --ignore-path .gitignore --write .",
    "generate:svg": "svgr --icon --typescript --out-dir src/components/Icons src/images/icons",
    "generate:tokens": "style-dictionary build --config src/style-dictionary.json",
    "generate": "npm-run-all --parallel generate:*",
    "lint:css": "stylelint --ignore-path .gitignore '**/*.css'",
    "lint:js": "eslint '**/*.{js,jsx,ts,tsx}'",
    "lint": "npm-run-all --parallel lint:*",
    "serve": "gatsby serve",
    "start": "npm run serve",
    "test": "jest --maxWorkers=50% --watch",
    "test:ci": "jest --ci --coverage --runInBand"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nicolebunge/nicolebunge.com"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@storybook/testing-react": "^1.3.0",
    "@svgr/cli": "^6.5.1",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^12.1.4",
    "@types/jest-axe": "^3.5.5",
    "@types/lodash": "^4.14.195",
    "@types/node": "^18.13.0",
    "@types/react": "^18.2.14",
    "@types/react-dom": "^17.0.11",
    "@types/react-helmet": "^6.1.6",
    "@typescript-eslint/eslint-plugin": "^5.61.0",
    "@typescript-eslint/parser": "^5.61.0",
    "eslint": "^8.44.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.6.0",
    "jest-axe": "^6.0.0",
    "jest-environment-jsdom": "^29.6.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.8",
    "style-dictionary": "^3.8.0",
    "stylelint": "^15.10.0",
    "stylelint-config-standard": "^34.0.0",
    "stylelint-order": "^6.0.3",
    "stylelint-selector-bem-pattern": "^3.0.0",
    "typescript": "^4.9.5"
  }
}
